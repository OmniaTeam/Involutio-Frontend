{
  "name": "Involutio",
  "short_name": "Involutio",
  "description": "Бесплатное веб-приложение для контроля деятельности сотрудников компании.",
  "lang": "ru-RU",
  "start_url": "https://involutio.the-omnia.ru",
  "scope": "/app/",
  "display": "standalone",
  "orientation": "any",
  "theme_color": "#000000",
  "background_color": "#000000",
  "icons": [
    {
      "src": "logo_maskable.png",
      "sizes": "150x150",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "logo_rounded.png",
      "sizes": "150x150",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "web_apps": [
    {
      "manifest": "/app/manifest.json",
      "details": {
        "paths": ["/*"]
      }
    }
  ],
  "serviceworker": {
    "src": "service-worker.js",
    "scope": "/"
  }
}