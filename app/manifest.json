{
  "name": "Involutio",
  "short_name": "Involutio",
  "description": "Бесплатное веб-приложение для контроля деятельности сотрудников компании.",
  "lang": "ru-RU",
  "start_url": "https://involutio.the-omnia.ru/",
  "scope": "https://involutio.the-omnia.ru/",
  "display": "standalone",
  "orientation": "any",
  "theme_color": "#000000",
  "icons": [
    {
      "purpose": "maskable",
      "src": "logo_maskable.svg",
      "sizes": "144x144"
    },
    {
      "purpose": "any",
      "src": "logo_rounded.svg",
      "sizes": "150x150"
    }
  ],
  "web_apps": [
    {
      "manifest": "/app/manifest.json",
      "details": {
        "paths": [ "/*" ]
      }
    }
  ],
  "serviceworker": {
    "src": "/service-worker.js",
    "scope": "/"
  }
}